#!/usr/bin/expect -f

set timeout 30
set password "Grad123!\$"

spawn ssh nbhtd@hellbender.rnet.missouri.edu "squeue -u nbhtd -n skiome_melsi -o '%.18i %.9P %.20j %.8u %.2t %.10M %.6D %R %S %e'"

expect {
    "password:" {
        send "$password\r"
        exp_continue
    }
    "Permission denied" {
        puts "Authentication failed"
        exit 1
    }
    eof
}

wait
